APXS=@APXS@
AP_SBINDIR=$(shell $(APXS) -q sbindir)
AP_PROGNAME=$(shell $(APXS) -q progname)
HTTPD=$(AP_SBINDIR)/$(AP_PROGNAME)
all: conf
test: test.conf
	prove -I.. .
conf: test.conf
test.conf: test.conf.tt *.t mkconf.pl
	./mkconf.pl --input $< --output $@
clean:
	rm -f test.conf
.PHONY: conf clean test
